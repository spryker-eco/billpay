{
  "name": "spryker-eco/billpay",
  "description": "Billpay Module",
  "autoload": {
    "psr-0": {
      "Spryker": "src/"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Acceptance": "tests/",
      "Functional": "tests/",
      "Unit": "tests/"
    },
    "psr-4": {
      "Billpay\\Module\\": "tests/",
      "Config\\Module\\": "vendor/spryker/config/tests/_support/Module",
      "Oms\\Module\\": "vendor/spryker/oms/tests/_support/Module",
      "Propel\\Module\\": "vendor/spryker/propel/tests/_support/Module",
      "Sales\\Module\\": "vendor/spryker/sales/tests/_support/Module",
      "Transfer\\Module\\": "vendor/spryker/transfer/tests/_support/Module"
    }
  },
  "require": {
    "billpay/lib": "1.6.0",
    "spryker/application": "^3.0.0",
    "spryker/config": "^3.0.0",
    "spryker/country": "^3.0.0",
    "spryker/currency": "^3.0.0",
    "spryker/gui": "^3.0.0",
    "spryker/kernel": "^3.0.0",
    "spryker/messenger": "^3.0.0",
    "spryker/oms": "^7.0.0",
    "spryker/payment": "^4.0.0",
    "spryker/propel": "^3.0.0",
    "spryker/refund": "^5.0.0",
    "spryker/sales": "^8.0.0",
    "spryker/step-engine": "^3.0.0",
    "spryker/symfony": "^3.0.0",
    "spryker/transfer": "^3.0.0",
    "spryker/url": "^3.0.0"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/customer": "^2.0.0 || ^3.0.0",
    "spryker/testify": "*"
  },
  "license": "proprietary",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}

